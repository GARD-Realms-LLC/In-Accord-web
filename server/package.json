{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "ts-node prisma/seed.ts",
    "migrate:seed-users": "ts-node scripts/migrate-seed-users.ts",
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node dist/src/index.js",
    "dev": "npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/index.ts\"",
    "prisma": "prisma",
    "dev:port": "node scripts/free-port-8000.cjs",
    "dev:freeport": "node scripts/free-port-8000.cjs --kill",
    "pm2:start": "npm run build && pm2 start dist/src/index.js --name inaccord-backend",
    "pm2:stop": "pm2 stop inaccord-backend",
    "pm2:restart": "pm2 restart inaccord-backend",
    "pm2:delete": "pm2 delete inaccord-backend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "body-parser": "^1.20.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.1",
    "nodemailer": "^6.9.3",
    "rimraf": "^6.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.14.10",
    "@types/nodemailer": "^7.0.4",
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.11",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "prisma": {
    "seed": "ts-node --esm prisma/seed.ts"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
